<?xml version="1.0" encoding="UTF-8"?>
<tsoftware
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<object name="resourceType" table="t_resource_type"
			primary="id" unique="" order="updateTime desc">
			<property name="id" type="Long" length="10" initial=""
				field="id" />
			<property name="name" type="String" length="50" initial=""
				field="name" />
			<property name="description" type="String" length="200"
				field="description" />
			<property name="picId" type="Long" length="10"
				field="pic_id" />
			<property name="picName" type="String" length="20" field="" />
			<property name="createTime" type="Date" length=""
				validate="" field="create_time" />
			<property name="createBy" type="Long" length="20"
				validate="" field="create_by" />
			<property name="updateTime" type="Date" length=""
				validate="" field="update_time" />
			<property name="updateBy" type="Long" length="20"
				validate="" field="update_by" />
			<property name="updateCount" type="Long" length="11"
				validate="" field="update_count" />
		</object>

	</model_layer>
</tsoftware>
