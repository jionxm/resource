<?xml version="1.0" encoding="UTF-8"?>
<tsoftware
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<!-- 课程表 -->
		<object name="expCourse" table="t_exp_course" primary="id" unique=""
			order="updateTime desc">
			<property name="id" type="Long" length="10" initial=""
				field="id" />
			<property name="courseId" type="Long" length="20"
				field="course_id" />
			<property name="systemName" type="String" length="20"
				field="system_name" />
			<property name="systemCode" type="String" length="200"
				field="system_code" />
			<property name="stageId" type="Long" length="20"
				field="stage_id" />
			<property name="versionName" type="String" length="20"
				field="version_name" />
			<property name="versionId" type="Long" length="20"
				field="version_id" />
			<property name="stageName" type="String" length="20"
				field="stage_name" />
			<property name="courseName" type="String" length="20"
				field="course_name" />
			<property name="updateTime" type="Date" length=""
				validate="" field="update_time" />
			<property name="updateCount" type="Long" length="20"
				validate="" field="update_count" />
		</object>
	</model_layer>
</tsoftware>
