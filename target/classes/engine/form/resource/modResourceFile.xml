<?xml version="1.0" encoding="UTF-8"?>
<tsoftware xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<object name="resourceFile" table="t_resource_file" primary="id"
			unique="" order="updateTime desc">
			<property name="id" type="Long" length="20" initial=""
				field="id" />
			<property name="resourceType" type="Long" length="50"
				initial="" field="resource_type" />
			<property name="fileId" type="Long" length="10" field="file_id" />
			<property name="name" type="String" length="10" field="name" />
			<property name="server" type="Long" length="10" field="server" />
			<property name="status" type="Long" length="10" field="status" />
			<property name="description" type="String" length="200"
				field="description" />
			<property name="createTime" type="Date" length="" validate=""
				field="create_time" />
			<property name="createBy" type="Long" length="20" validate=""
				field="create_by" />
			<property name="updateTime" type="Date" length="" validate=""
				field="update_time" />
			<property name="updateBy" type="Long" length="20" validate=""
				field="update_by" />
			<property name="updateCount" type="Long" length="11"
				validate="" field="update_count" />
			<property name="length" type="String" length="20" validate=""
				field="" />
			<property name="fileIdName" type="String" length="50" field="" />
			<property name="resourceTypeName" type="String" length="50"
				field="" />
			<property name="serverName" type="String" length="50" field="" />
			<property name="createByName" type="String" length="50"
				field="" />
			<property name="updateByName" type="String" length="50"
				field="" />
			<property name="ctlfile" type="String" length="50" field="" />
			<property name="courseName" type="String" length="20"
				validate="" field="" />
			<property name="courseId" type="Long" length="10" initial=""
				field="course_id" />
			<property name="statusName" type="String" length="50" field="" />
		</object>

	</model_layer>
</tsoftware>
